<div class="squatch-share">

  <div class="form-group">
    <div class="contacts-importer">
      Import contacts: &nbsp;&nbsp;
      <a class="cloudsponge-launch" data-cloudsponge-source="gmail">
        <img src="{{assets 'images/contacts/gmail.png'}}">
        Gmail
      </a> &nbsp; | &nbsp;
      <a class="cloudsponge-launch" data-cloudsponge-source="yahoo">
        <img src="{{assets 'images/contacts/yahoo.png'}}">
        Yahoo! Mail
      </a> &nbsp; | &nbsp;
      <a class="cloudsponge-launch" data-cloudsponge-source="windowslive">
        <img src="{{assets 'images/contacts/windowslive.png'}}">
        Outlook
      </a>
    </div>
    <div class="input-group">
      <input class="form-control cloudsponge-contacts" id="contact-list">
      <span class="input-group-btn">
        <button class="btn btn-primary disabled" id="squatch-send-invites">Send Invites</button>
      </span>
    </div>
  </div>

  <div class="clearfix">
    <a class="btn btn-email squatch-share-btn emailShare {{variables 'widget.REFERRER_WIDGET.content.sharingOptions.EMAIL.emailDisplayRule'}}"
       target="{{#if (stringCompare mode "POPUP" emptyOnFalse=true)}}_blank{{else}}{{#if (stringCompare mode "EMBED" emptyOnFalse=true)}}_blank{{else}}_self{{/if}}{{/if}}">
      <i class="icon icon-mail"></i>
    </a>

    <a class="btn btn-twitter squatch-share-btn twShare {{variables 'widget.REFERRER_WIDGET.content.sharingOptions.TWITTER.twitterDisplayRule'}}"
       target="{{#if (stringCompare mode "POPUP" emptyOnFalse=true)}}_blank{{else}}{{#if (stringCompare mode "EMBED" emptyOnFalse=true)}}_blank{{else}}_self{{/if}}{{/if}}">
      <i class="icon icon-twitter"></i>
    </a>

    <a class="btn btn-twitter squatch-share-btn twMobile {{variables 'widget.REFERRER_WIDGET.content.sharingOptions.TWITTER.twitterDisplayRule'}}"
       target="{{#if (stringCompare mode "POPUP" emptyOnFalse=true)}}_blank{{else}}{{#if (stringCompare mode "EMBED" emptyOnFalse=true)}}_blank{{else}}_self{{/if}}{{/if}}">
      <i class="icon icon-twitter"></i>
    </a>

    <a class="btn btn-sms squatch-share-btn smsShare {{variables 'widget.REFERRER_WIDGET.content.sharingOptions.SMS.smsDisplayRule'}}"
       target="{{#if (stringCompare mode "POPUP" emptyOnFalse=true)}}_blank{{else}}{{#if (stringCompare mode "EMBED" emptyOnFalse=true)}}_blank{{else}}_self{{/if}}{{/if}}">
      <i class="icon icon-chat"></i>
    </a>

    <a class="btn btn-messenger squatch-share-btn messengerShare {{variables 'widget.REFERRER_WIDGET.content.sharingOptions.FBMESSENGER.facebookDisplayRule'}}"
       target="{{#if (stringCompare mode "POPUP" emptyOnFalse=true)}}_blank{{else}}{{#if (stringCompare mode "EMBED" emptyOnFalse=true)}}_blank{{else}}_self{{/if}}{{/if}}">
      <i class="icon icon-messenger"></i>
    </a>

    <a class="btn btn-facebook squatch-share-btn fbShare {{variables 'widget.REFERRER_WIDGET.content.sharingOptions.FACEBOOK.facebookDisplayRule'}}"
       target="{{#if (stringCompare mode "POPUP" emptyOnFalse=true)}}_blank{{else}}{{#if (stringCompare mode "EMBED" emptyOnFalse=true)}}_blank{{else}}_self{{/if}}{{/if}}">
      <i class="icon icon-facebook"></i>
    </a>

    <a class="btn btn-facebook squatch-share-btn fbMobile {{variables 'widget.REFERRER_WIDGET.content.sharingOptions.FACEBOOK.facebookDisplayRule'}}"
       target="{{#if (stringCompare mode "POPUP" emptyOnFalse=true)}}_blank{{else}}{{#if (stringCompare mode "EMBED" emptyOnFalse=true)}}_blank{{else}}_self{{/if}}{{/if}}">
      <i class="icon icon-facebook"></i>
    </a>

    <a class="btn btn-whatsapp squatch-share-btn whatsappShare {{variables 'widget.REFERRER_WIDGET.content.sharingOptions.WHATSAPP.whatsappDisplayRule'}}"
       target="{{#if (stringCompare mode "POPUP" emptyOnFalse=true)}}_blank{{else}}{{#if (stringCompare mode "EMBED" emptyOnFalse=true)}}_blank{{else}}_self{{/if}}{{/if}}">
      <i class="icon icon-whatsapp"></i>
    </a>

    <div class="copy-share-box">
      <div class="input-group">
        <input class="form-control" id="squatch-share-link" value="{{shareLinks.shareLink}}" readonly="readonly">

        <span class="label label-default fade" id="squatch-share-notification">Copied!</span>

        <span class="input-group-btn">
          <button class="btn btn-primary icon-btn copy" data-clipboard-target="#squatch-share-link" data-clipboard-notification="#squatch-share-notification">
            Copy
          </button>
        </span>
      </div>
    </div>

  </div>
</div>
